<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head><title>CITYNAME Rapid Transit Timeline</title>
<meta http-equiv="Content-type" content="text/html;charset=UTF-8">
<script language="JavaScript" type="text/javascript">
start=START;
count=COUNT;
step=STEP;
index=count-1;
function update() {
	document["MAP"].src=(start+step*index) + ".png";
	document["MAP"].title=start+step*index;
	document["MAP"].alt=start+step*index + " map";
	document.getElementsByTagName('a')[2].href=((start+step*index) + ".svg");
}
function nextmap() {
	index=(index+1)%count;
	update();
}
function prevmap() {
	index=(index+count-1)%count;
	update();
}
document.onkeydown=function(keypress) {
	if(keypress.which == 65) { prevmap(); }
	if(keypress.which == 83) { nextmap(); }
}
</script>
<script type="text/javascript">
  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-19998781-1']);
  _gaq.push(['_trackPageview']);
  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
</script>
